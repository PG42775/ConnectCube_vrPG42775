    )
    (shape
      (circle In2.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle In3.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle In4.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
