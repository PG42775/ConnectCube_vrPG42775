  )
  (via_rule
    "kicad_default" "Via[0-5]_800:400_um-kicad_default"
  )
  (via_rule
    3.3V "Via[0-5]_800:400_um-3.3V"
  )
  (via_rule
    5V "Via[0-5]_800:400_um-5V"
